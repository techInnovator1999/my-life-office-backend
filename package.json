{
  "name": "life-agent-portal",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create:relational": "hygen seeds create-relational",
    "seed:create:document": "hygen seeds create-document",
    "seed:run:relational": "ts-node -r tsconfig-paths/register ./src/database/seeds/relational/run-seed.ts",
    "seed:run:document": "ts-node -r tsconfig-paths/register ./src/database/seeds/document/run-seed.ts",
    "create:admin": "ts-node -r tsconfig-paths/register ./src/scripts/create-admin.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start-dev-server": "npm install && npm run migration:run && npm run seed:run:relational && npm run start:dev",
    "start-prod-server": "npm run migration:run && npm run seed:run:relational && npm run start:prod",
    "start:dev": "nest start --watch",
    "start:swc": "nest start -b swc -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" && tsc --noEmit",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install",
    "docker-compose:local:run": "docker compose -f docker-compose.2.0.local.yml up",
    "docker-compose:local:build": "docker compose -f docker-compose.2.0.local.yml up --build",
    "docker-compose:local:down": "docker compose -f docker-compose.2.0.local.yml down",
    "stripe:local:webhook:listen": "node -e \"require('dotenv').config(); const { execSync } = require('child_process'); execSync('docker run --rm -it --network=host stripe/stripe-cli:latest listen --api-key ' + process.env.STRIPE_SECRET_KEY + ' --forward-to http://host.docker.internal:' + process.env.APP_PORT + '/api/v1/stripe/webhook/account-event', {stdio: 'inherit'});\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.782.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "11.0.13",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.13",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.13",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "11.1.1",
    "@nestjs/typeorm": "11.0.0",
    "@sentry/nestjs": "^9.11.0",
    "@sentry/profiling-node": "^9.11.0",
    "@types/multer-s3": "3.0.3",
    "apple-signin-auth": "1.7.8",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "bcryptjs": "3.0.2",
    "bson": "^6.10.3",
    "bullmq": "^5.46.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "^16.4.7",
    "fb": "2.0.0",
    "fcm-node": "^1.3.0",
    "firebase-admin": "^13.2.0",
    "google-auth-library": "9.15.1",
    "handlebars": "4.7.8",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.48",
    "mongoose": "^8.13.2",
    "ms": "2.1.3",
    "multer": "1.4.4",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.5.1",
    "nodemailer": "6.10.0",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "pg": "8.14.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "source-map-support": "0.5.21",
    "stripe": "^18.0.0",
    "swagger-ui-express": "5.0.1",
    "twitter": "^1.1.0",
    "typeorm": "0.3.22",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@nestjs/cli": "11.0.6",
    "@nestjs/schematics": "11.0.3",
    "@nestjs/testing": "11.0.13",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.16",
    "@types/bcryptjs": "3.0.0",
    "@types/express": "5.0.1",
    "@types/facebook-js-sdk": "3.3.12",
    "@types/jest": "29.5.14",
    "@types/ms": "2.1.0",
    "@types/multer": "1.4.12",
    "@types/node": "22.14.0",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.3",
    "@types/twitter": "1.7.4",
    "@types/uuid": "^10.0.0",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "env-cmd": "10.1.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.6",
    "husky": "9.1.7",
    "hygen": "6.2.11",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "ts-jest": "29.3.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typescript": "5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
